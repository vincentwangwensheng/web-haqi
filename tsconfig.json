{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "paths": {
      "@fuse": [
        "@fuse/"
      ],
      "ng-material-flatpickr": [
        "dist/ng-material-flatpickr"
      ],
      "ng-material-flatpickr/*": [
        "dist/ng-material-flatpickr/*"
      ]
    },
    "target": "es6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": false,
    "strictInjectionParameters": false
  }
}